## comandos git
PATH_ALIAS_GIT="/var/www/scripts/git_alias"
alias gitcommit="${PATH_ALIAS_GIT}/gitAddCommit.sh"
alias gitpull="${PATH_ALIAS_GIT}/gitPull.sh"
alias gitpush="${PATH_ALIAS_GIT}/gitPush.sh"
alias gitrestore="${PATH_ALIAS_GIT}/gitResetBranch.sh"
alias gits="${PATH_ALIAS_GIT}/gitStatus.sh"
alias gitl="${PATH_ALIAS_GIT}/gitLog.sh"